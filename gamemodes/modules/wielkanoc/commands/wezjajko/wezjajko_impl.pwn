command_wezjajko_Impl(playerid)
{
    new i = EasterEggs_FindNearest(playerid);
    if(i != INVALID_EGG_ID) EasterEggs_Pickup(playerid, i);
    else SendClientMessage(playerid, COLOR_RED, "[EasterEgg]Nie znaleziono jajka w pobli¿u.");
    return 1;
}

