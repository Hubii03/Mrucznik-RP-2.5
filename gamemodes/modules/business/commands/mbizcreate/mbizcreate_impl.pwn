//------------------<[ Implementacja: ]>-------------------
command_mbizcreate_Impl(playerid, bCost, bName[64])
{
    if(!IsAHeadAdmin(playerid))
    {
        noAccessMessage(playerid); 
        return 1;
    }
    CreateMBiz(playerid, bCost, bName); 
    return 1;
}

