command_mbizpanel_Impl(playerid)
{
    if(PlayerInfo[playerid][pBusinessOwner] == INVALID_BUSINESSID)
    {
        sendErrorMessage(playerid, "Nie posiadasz w³asnego biznesu!"); 
        return 1;
    }
    ShowBusinessOwnerDialog(playerid); 
    return 1;
}

